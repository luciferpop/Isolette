  <requirementGroups id="1b08e51d-3401-4190-8afc-d94c99cb1cd9" name="A.5 Thermostat High Level" description="These are the high level requirements for the thermostat, which are the decomposition of the Isolette security requiremeents SR1 and SR2. The Thermostat performs two logically independent functions. The first regulates the Current Temperature in the Isolette so it is maintained within the Desired Temperature Range. The second monitors the Current Temperature in the Isolette and activates an Alarm if it falls below or rises above the Alarm Temperature Range.">
    <requirements xsi:type="core:Requirement" id="7124c0c0-0fba-428d-8208-c8409ccecbd9" name="REQ-TH-1: Set value of Heat Control" description="The Thermostat shall set the value of the Heat Control." rationale="A primary function of the Thermostat is to turn the Heat Control on and off to maintain the Current Temperature in the Isolette within the Desired Temperature Range, which is required by SR-1." containedRequirements="core:Requirement ../regulate_temperature/regulate_temperature.rdal#89a1569d-70cd-424a-a672-666280d4f8ef" category="platform:/config/fr.openpeople.ide.settings/default.settings#cat_System_Function">
      <satisfiedBy xsi:type="core:RefUserSelectedModelElements" id="14575a9b-b269-49d9-ad81-bff2e0cf6f0f">
        <modelElementReferences id="fdce936b-72d0-4acf-863b-e4f09a0583a9" modelElement="isolette_thermostat.aadl#isolette_thermostat.Thermostat"/>
      </satisfiedBy>
      <expression xsi:type="cst:NaturalLanguageExpression" id="da34bc81-5634-4f30-81f3-1672fb438699" language="cst:NaturalConstraintLanguage platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#fr.labsticc.framework.constraints.core.natural" text="The Thermostat shall set the value of the Heat Control."/>
    </requirements>
    <requirements xsi:type="core:Requirement" id="37da9cc2-cb77-4112-8c38-d26b28eacdf2" name="REQ-TH-2: Set value of Regulator Status" description="The Thermostat Function shall set the value of the Regulator Status." stakeHolders="../../isolette/isolette/isolette.rdal#0b915e70-bb90-4791-b91e-a3a00604b739" rationale="SR-1 requires the Thermostat to provide an independent regulator function. The status of this function is provided to the Operator Interface by the Thermostat. The Operator Interface will use the Regulator Status and the Monitor Status to report the overall status of the Thermostat, which is required by SR-1." containedRequirements="core:Requirement ../regulate_temperature/regulate_temperature.rdal#ad1b6077-795f-4edf-a94b-6d6087c940ae" category="platform:/config/fr.openpeople.ide.settings/default.settings#cat_System_Function">
      <satisfiedBy xsi:type="core:RefUserSelectedModelElements" id="7ca8c91c-4038-4b24-bef6-86fe914ec6eb">
