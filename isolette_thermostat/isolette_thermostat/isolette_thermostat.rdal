      <satisfiedBy xsi:type="core:RefUserSelectedModelElements" id="3e6650ae-9a7d-4af1-9aea-c3481f27f84f">
        <modelElementReferences id="7579ac4d-0e0c-4ea5-b6f1-7e756dd9bcd8" modelElement="../../isolette/isolette/isolette.aadl#isolette.Operator_Interface"/>
      </satisfiedBy>
      <expression xsi:type="cst:FormalLanguageExpression" id="a36ab538-eb5f-4d77-b4e5-cc9a33f5c453" language="cst:FormalConstraintLanguage platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#fr.labsticc.framework.constraints.bless" text="false"/>
