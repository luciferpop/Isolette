      <expression xsi:type="cst:FormalLanguageExpression" id="376920a8-21a7-474a-8edd-1b8048f0e40f" language="cst:FormalConstraintLanguage platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#fr.labsticc.framework.constraints.bless" text="false"/>
    </requirements>
    <requirements xsi:type="core:Assumption" id="5f39c94e-6dcd-45ff-b6c7-837f471fca7f" name="EA-OI-6: Lower Desired Temp &lt;= Upper Desired Temp of -1°F." description="The Lower Desired Temperature will always be less than or equal to the Upper Desired Temperature of -1°F." rationale="If the Lower Desired Temperature is greater than or equal to the Upper Desired Temperature, it is unclear if the Heat Source should be on or off. This may result in excessive cycling of the Heat Source." requirements="../../isolette/isolette/isolette.rdal#44607377-2cf3-4205-a244-b74673f943e2">
      <satisfiedBy xsi:type="core:RefUserSelectedModelElements" id="eb4c95c3-e548-475e-982f-df7a7dce4a7f">
        <modelElementReferences id="bdf8ce23-611e-47a4-917e-c099bc522d09" modelElement="../../isolette/isolette/isolette.aadl#isolette.Operator_Interface"/>
      </satisfiedBy>
      <expression xsi:type="cst:FormalLanguageExpression" id="be889aef-ef8d-4e6e-b33e-35fb3a238b9a" language="cst:FormalConstraintLanguage platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#fr.labsticc.framework.constraints.bless" text="false"/>
    </requirements>
    <requirements xsi:type="core:Assumption" id="d070f05f-886c-4c72-a82e-6464df8aabe4" name="EA-OI-7: Upper Desired Temp ≤ 100°F" description="The Upper Desired Temperature will always be ≤100°F." rationale="Exposing the Infant to temperatures greater than 100°F may result in an incorrect diagnosis of fever resulting in aggressive evaluation (blood culture and lumbar puncture) and treatment for infection." requirements="../../isolette/isolette/isolette.rdal#e0a36804-6713-4de2-aba1-acdf72b97619">
      <satisfiedBy xsi:type="core:RefUserSelectedModelElements" id="98723f61-e72d-417a-b03b-b019d4aedd2f">
        <modelElementReferences id="ed83fdce-72eb-4beb-b957-99280c392ddc" modelElement="../../isolette/isolette/isolette.aadl#isolette.Operator_Interface"/>
      </satisfiedBy>
      <expression xsi:type="cst:FormalLanguageExpression" id="43e88814-1a89-4d78-8126-86741dde7568" language="cst:FormalConstraintLanguage platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#fr.labsticc.framework.constraints.bless" text="false"/>
    </requirements>
    <requirements xsi:type="core:Assumption" id="24cb587a-2fc3-4a60-af96-ccc48b7fac8a" name="EA-OI-8: Upper Alarm Temp >= Upper Desired Temp of 1°F" description="The Upper Alarm Temperature will always be greater than or equal to the Upper Desired Temperature of 1°F." rationale="If the Upper Alarm Temperature is less than or equal to the Upper Desired Temperature, the Alarm could be activated while the Current Temperature is still in the Desired Temperature Range." requirements="../../isolette/isolette/isolette.rdal#5c1e62a2-d1a2-4a53-acc6-1e6b4df5f61d">
      <satisfiedBy xsi:type="core:RefUserSelectedModelElements" id="efa6a655-1db3-4169-a91f-c2b0924eada0">
        <modelElementReferences id="f187cd21-efb7-4fa3-83fb-9c8de5568724" modelElement="../../isolette/isolette/isolette.aadl#isolette.Operator_Interface"/>
      </satisfiedBy>
      <expression xsi:type="cst:FormalLanguageExpression" id="87457d8f-3a1c-45df-85a6-6c7c9eab7c14" language="cst:FormalConstraintLanguage platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#fr.labsticc.framework.constraints.bless" text="false"/>
    </requirements>
    <requirements xsi:type="core:Assumption" id="31c32c72-367f-44d9-823e-3de36a159dea" name="EA-OI-9: Upper Alarm Temp ≤ 103°F" description="The Upper Alarm Temperature will always be ≤103°F." rationale="Exposure to temperatures greater than 103°F will result in hyperthermia, which can lead to cardiac arrhythmias and febrile seizures within a few minutes." requirements="../../isolette/isolette/isolette.rdal#c33ce718-5237-4fc0-8b16-55e37f41a792">
