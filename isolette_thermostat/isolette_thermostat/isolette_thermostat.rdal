  <requirementGroups id="a6fb6168-37e9-4868-b5b0-4d625f88c654" name="Env Assumptions Isolette" description="These are the environmental assumptions made by the Thermostat about the Isolette.">
    <requirements xsi:type="core:Assumption" id="250ad6f9-d0d4-47c5-8703-303e2b725613" name="EA-IS-1: Current Temp increases no more than 1°F per min when Heat Source On" description="When the Heat Source is turned on and the Isolette is properly shut, the Current Temperature will increase at a rate of no more than 1°F per minute." rationale="If the Current Temperature can increase at a rate of more than 1°F per minute, the Thermostat may not be able to turn the Heat Source off quickly enough to maintain the Desired Temperature Range unless the allowed latency specified for the Heat Control is reduced." requirements="../../isolette/isolette/isolette.rdal#40b5d9ca-ce29-48f0-a2ac-de035d1dd3a0">
      <satisfiedBy xsi:type="core:RefUserSelectedModelElements" id="46e7fa29-ef21-4ccc-9874-52123218244c">
        <modelElementReferences id="1c0fabd0-2fbf-4b3c-8fc3-ee189d91a7c0" modelElement="../../isolette/isolette/isolette.aadl#isolette.Isolette"/>
      </satisfiedBy>
      <expression xsi:type="cst:FormalLanguageExpression" id="4d641ea1-1f62-4a5b-aefd-6999c57dd671" language="cst:FormalConstraintLanguage platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#fr.labsticc.framework.constraints.lute" text="Property( Self, &quot;Isolette_Properties::Temperature_Increase_Rate&quot;, &quot;Fdeg_Minute&quot; ) &lt;= 1.0" usedLibraries="platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#platform:/config/fr.openpeople.ide.settings/lute/default/libStandard.lute"/>
    </requirements>
    <requirements xsi:type="core:Assumption" id="b45abdf5-bc2b-47a9-a09b-187e8b659598" name="EA-IS-2: Current Temp decreases no more than 1°F per min when Heat Source Off" description="When the Heat Source is turned off and the Isolette is properly shut, the Current Temperature will decrease at a rate of no more than 1°F per minute." rationale="If the Current Temperature can decrease at a rate of more than 1°F per minute, the Thermostat may not be able to turn the Heat Source on quickly enough to maintain the Desired Temperature Range unless the allowed latency specified for the Heat Control is reduced." requirements="../../isolette/isolette/isolette.rdal#49c2b7eb-d561-4336-a9c2-38816e191026">
      <satisfiedBy xsi:type="core:RefUserSelectedModelElements" id="525881e7-5806-4d01-9a28-07edc7ab0031">
        <modelElementReferences id="827fc193-62f5-4bac-856f-2bcb1135e891" modelElement="../../isolette/isolette/isolette.aadl#isolette.Isolette"/>
      </satisfiedBy>
      <expression xsi:type="cst:FormalLanguageExpression" id="7180318d-880f-41d6-abd8-ddf05f755e7c" language="cst:FormalConstraintLanguage platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#fr.labsticc.framework.constraints.lute" text="Property( Self, &quot;Isolette_Properties::Temperature_Decrease_Rate&quot;, &quot;Fdeg_Minute&quot; ) &lt;= 1.0" usedLibraries="platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#platform:/config/fr.openpeople.ide.settings/lute/default/libStandard.lute"/>
    </requirements>
  </requirementGroups>
  <requirementGroups id="85791e2b-ea99-4ff2-ac68-73731d1c5bb8" name="Env Assumptions Temperature Sensor" description="These are the environmental assumptions are made by the Thermostat about the Temperature Sensor.">
    <requirements xsi:type="core:Assumption" id="c551e704-4236-4ce2-97d6-515c1c1db3c3" name="EA-TS-1: Current Temp in °F" description="The Current Temperature will be provided to the Thermostat in °F." rationale="Consistency with environmental-assumption Operator Interface EA-OI-1" requirements="../../isolette/isolette/isolette.rdal#bbd0040d-7e64-4324-8bd8-3913aafba46d">
      <satisfiedBy xsi:type="core:RefUserSelectedModelElements" id="e609a595-9cf2-4914-807a-bf44ac24087a">
        <modelElementReferences id="a6a2e152-eb58-4ca2-b71e-0deee00b9d9a" modelElement="../../isolette/isolette/isolette.aadl#isolette.Current_Temp_Value"/>
      </satisfiedBy>
      <expression xsi:type="cst:FormalLanguageExpression" id="7b7a700a-82a1-4b71-961c-70b25cf8ffd8" language="cst:FormalConstraintLanguage platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#fr.labsticc.framework.constraints.lute" text="Property( Self, &quot;Data_Model::Measurement_Unit&quot; ) = &quot;Fahrenheit&quot;" usedLibraries="platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#platform:/config/fr.openpeople.ide.settings/lute/default/libStandard.lute"/>
    </requirements>
    <requirements xsi:type="core:Assumption" id="d4712b5d-24e5-4923-8f26-a9213b7525ba" name="EA-TS-2: Current Temp Sensed to ±0.1°F" description="The Current Temperature will be sensed to an accuracy of ±0.1°F." rationale="An accuracy of 0.1°F is necessary to ensure the Thermostat can turn the Heat Source on and off quickly enough to maintain the Desired Temperature Range." requirements="../../isolette/isolette/isolette.rdal#6a06a4a5-6385-4bc8-b766-5f5af9bb7552">
      <satisfiedBy xsi:type="core:RefUserSelectedModelElements" id="61be3340-de39-407c-89bb-4e5fd5d4903f">
        <modelElementReferences id="5a235d65-1da6-4111-b008-7c2dc1fdeb8f" modelElement="../../isolette/isolette/isolette.aadl#isolette.Current_Temp_Value"/>
      </satisfiedBy>
      <expression xsi:type="cst:FormalLanguageExpression" id="7afdb182-f76a-440e-ad19-9804c040e8ce" language="cst:FormalConstraintLanguage platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#fr.labsticc.framework.constraints.lute" text="Property( Self, &quot;Isolette_Properties::Temperature_Tolerance&quot;, &quot;Fahrenheit&quot; ) &lt;= 0.1" usedLibraries="platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#platform:/config/fr.openpeople.ide.settings/lute/default/libStandard.lute"/>
    </requirements>
    <requirements xsi:type="core:Assumption" id="513e6cb6-16fe-4cd4-8b60-1b9450391e08" name="EA-TS-3: Current Temp Range of at least 68.0° to 105.0°F" description="The Current Temperature will cover the range of at least 68.0° to 105.0°F." rationale="This is the specified range of operation of the Isolette. The lower end of this range is useful for monitoring an Isolette that is warming to the Desired Temperature Range. The upper end is greater than the Upper Alarm Temperature to ensure that the Current Temperature will be sensed across the entire Alarm Temperature Range." requirements="../../isolette/isolette/isolette.rdal#df9d6554-1e5b-48f8-9ed1-56d2a93ba0b5">
      <satisfiedBy xsi:type="core:RefUserSelectedModelElements" id="2c14b300-9332-44a4-9fc6-332180bb6eac">
        <modelElementReferences id="9bba5471-122f-4bea-8465-5048eb06fcac" modelElement="../../isolette/isolette/isolette.aadl#isolette.Current_Temp_Value"/>
      </satisfiedBy>
      <expression xsi:type="cst:FormalLanguageExpression" id="0077a402-3c1a-47c5-b96d-77e7df1a1647" language="cst:FormalConstraintLanguage platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#fr.labsticc.framework.constraints.lute" text="Lower( Property( Self, &quot;Data_Model::Real_Range&quot; ) ) &lt;= 68.0 and Upper( Property( Self, &quot;Data_Model::Real_Range&quot; ) ) >= 105.0" usedLibraries="platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#platform:/config/fr.openpeople.ide.settings/lute/default/libStandard.lute"/>
    </requirements>
    <requirements xsi:type="core:Assumption" id="484f1583-2536-4196-a880-066718485f22" name="EA-TS-4: Current Temp Status INVALID when Temp Sensor Malfunction" description="The status of the Current Temperature variable should be INVALID when the Temperature Sensor does not function properly (this assumption was not in the FAA example but is added because requirement MRM-3 relies on the Regulator Status variable which depend on the status of the Current Temperature variable)." rationale="This is because requirements MRM-2 and MRM-3 rely on the Regulator Status variable which depend on the status of the Current Temperature variable.">
      <satisfiedBy xsi:type="core:RefUserSelectedModelElements" id="d36de143-14c5-455e-9fd8-ee0bfce29446">
        <modelElementReferences id="a4bc890e-336c-42a1-94f1-4fc56890d811" modelElement="../../isolette/isolette/isolette.aadl#isolette.Temperature_Sensor"/>
      </satisfiedBy>
      <expression xsi:type="cst:FormalLanguageExpression" id="d5aa11f8-608a-4319-981d-15560316cab4" language="cst:FormalConstraintLanguage platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#fr.labsticc.framework.constraints.bless" text="false"/>
    </requirements>
    <requirements xsi:type="core:Assumption" id="8de09b1d-6d56-4780-acdd-849b8f72dc65" name="EA-TS-5: Current Temp Status VALID when Temp Sensor Function Correctly" description="The status of the Current Temperature variable should be VALID when the Temperature Sensor functions properly (this assumption was not in the FAA example but is added because requirement MRM-2 relies on the Regulator Status variable which depend on the status of the Current Temperature variable).">
      <satisfiedBy xsi:type="core:RefUserSelectedModelElements" id="9565cfb0-4257-405a-aa07-97eee5d1601c">
        <modelElementReferences id="c3fe2a50-967c-44b6-a642-2f536f843448" modelElement="../../isolette/isolette/isolette.aadl#isolette.Temperature_Sensor"/>
      </satisfiedBy>
      <expression xsi:type="cst:FormalLanguageExpression" id="67e397e0-9bb6-4279-acbe-5fbfa35e4ecf" language="cst:FormalConstraintLanguage platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#fr.labsticc.framework.constraints.bless" text="false"/>
    </requirements>
  </requirementGroups>
  <requirementGroups id="caca0365-6aa5-4c24-90ac-f2dcc7e35373" name="Env Assumptions Operator Interface" description="These are the environmental assumptions are made by the Thermostat about the Operator Interface. They are quite strong, which simplifies the manage Operator Interface Function. If these assumptions were not satisfied by the Operator Interface external entity, the Manage Operator Interface Function would need to be strengthened to ensure consistent inputs to the Thermostat.">
    <requirements xsi:type="core:Assumption" id="615be7ec-9e48-469c-8e32-c0f9e513f5e2" name="EA-OI-1: Temp Displayed in °F" description="All temperatures will be entered and displayed in degrees Fahrenheit." rationale="Minimize the complexity of this example. An actual system would probably support Celsius or perhaps both Fahrenheit and Celsius." requirements="../../isolette/isolette/isolette.rdal#aa9571cd-aa41-4645-979f-4aae714b9608">
      <satisfiedBy xsi:type="core:RefUserSelectedModelElements" id="0725ff7e-9932-442c-b92c-beeeb1e00a82">
