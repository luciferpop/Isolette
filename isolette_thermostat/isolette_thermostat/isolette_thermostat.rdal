      </satisfiedBy>
      <expression xsi:type="cst:FormalLanguageExpression" id="14ee198e-6d51-4577-b85a-2915bac1384f" language="cst:FormalConstraintLanguage platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#fr.labsticc.framework.constraints.bless" text="false"/>
