      <expression xsi:type="cst:FormalLanguageExpression" id="48706d3b-80f5-473b-af13-aa3923e61a58" language="cst:FormalConstraintLanguage platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#fr.labsticc.framework.constraints.bless" text="false"/>
