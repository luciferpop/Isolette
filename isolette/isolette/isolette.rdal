  <requirementGroups id="f622aa48-6a66-4cca-85ec-71e1a6a6700f" name="Isolette Performances" description="A group of performance requirements for the Isolette.">
    <requirements xsi:type="core:Requirement" id="a8697c5e-0b15-41d8-84eb-6b8f03f38870" name="REQ-IS-1: Current Temp within Desired Range with ±0.2°F tolerance" description="The temperature inside the Isolette shall be maintained within the Desired Range with a tolerance of 0.2 °F." stakeHolders="#d7b89361-3b40-4aa1-8c70-e9cf58663ffb" rationale="Actually, the rationale for this requirement is not presented in the example, but the assumed cool off rate of the Isolette with the required latency of turning the Heat Source on and off will ensure this tolerance." containedRequirements="#40b5d9ca-ce29-48f0-a2ac-de035d1dd3a0 #49c2b7eb-d561-4336-a9c2-38816e191026 #af492b68-4c1c-43f2-8d0f-45f850cb1332 #6a06a4a5-6385-4bc8-b766-5f5af9bb7552" category="platform:/config/fr.openpeople.ide.settings/default.settings#cat_Performance">
      <satisfiedBy xsi:type="core:RefUserSelectedModelElements" id="4a1dc146-fb19-48e7-bf53-53d7240715f0">
        <modelElementReferences id="93731ab5-00cc-4921-8d99-19988125184c" modelElement="isolette.aadl#isolette::Isolette"/>
      </satisfiedBy>
      <expression xsi:type="cst:NaturalLanguageExpression" id="7f3de6a4-e027-4fe1-ac39-781b6d63f78a" language="cst:NaturalConstraintLanguage platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#fr.labsticc.framework.constraints.core.natural" text="The temperature inside the Isolette shall be maintained constant with an accuracy of 0.1 °F."/>
    </requirements>
    <requirements xsi:type="core:Requirement" id="49c2b7eb-d561-4336-a9c2-38816e191026" name="REQ-IS-2: Current Temp Decr no more than 1°F per min when Heat Source Off" description="When the Heat Source is turned off and the Isolette is properly shut, the Current Temperature will decrease at a rate of no more than 1°F per minute." rationale="If the Current Temperature can decrease at a rate of more than 1°F per minute, the Thermostat may not be able to turn the Heat Source on quickly enough to maintain the Desired Temperature Range unless the allowed latency specified for the Heat Control is reduced." containedRequirements="#37dc6ceb-2dec-4805-9a1a-381be8c42688 #9a7cf2e7-e6df-4f44-aa83-f148687661f4" assumptions="../../isolette_thermostat/isolette_thermostat/isolette_thermostat.rdal#b45abdf5-bc2b-47a9-a09b-187e8b659598" category="platform:/config/fr.openpeople.ide.settings/default.settings#cat_Performance">
      <satisfiedBy xsi:type="core:RefUserSelectedModelElements" id="e9c781e7-288c-4d42-9692-28d8d2b9bf66">
        <modelElementReferences id="5615c085-a4e9-4065-a55e-4312e7d349e6" modelElement="isolette.aadl#isolette::Isolette"/>
      </satisfiedBy>
      <expression xsi:type="cst:FormalLanguageExpression" id="c49ed2c9-02e5-4d3a-9dd6-7a8b679450ac" language="cst:FormalConstraintLanguage platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#fr.labsticc.framework.constraints.lute" text="Property( Self, &quot;Isolette_Properties::Temperature_Increase_Rate&quot;, &quot;Fdeg_Minute&quot; ) &lt; 1.0" usedLibraries="platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#platform:/config/fr.openpeople.ide.settings/lute/default/libStandard.lute"/>
    </requirements>
    <requirements xsi:type="core:Requirement" id="40b5d9ca-ce29-48f0-a2ac-de035d1dd3a0" name="REQ-IS-3: Current Temp Incr no more than 1°F per min when Heat Source On" description="When the Heat Source is turned on and the Isolette is properly shut, the Current Temperature will increase at a rate of no more than 1°F per minute." rationale="If the Current Temperature can increase at a rate of more than 1°F per minute, the Thermostat may not be able to turn the Heat Source off quickly enough to maintain the Desired Temperature Range unless the allowed latency specified for the Heat Control is reduced. " containedRequirements="#37dc6ceb-2dec-4805-9a1a-381be8c42688 #c80dd8eb-db45-493c-b93c-20d745c67295 #9a7cf2e7-e6df-4f44-aa83-f148687661f4" assumptions="../../isolette_thermostat/isolette_thermostat/isolette_thermostat.rdal#250ad6f9-d0d4-47c5-8703-303e2b725613" category="platform:/config/fr.openpeople.ide.settings/default.settings#cat_Performance">
      <satisfiedBy xsi:type="core:RefUserSelectedModelElements" id="6978fc92-532a-41a5-951d-85e00b61241f">
        <modelElementReferences id="bc7e7cc3-7e1d-4aec-946a-e1e218c7af6c" modelElement="isolette.aadl#isolette::Isolette"/>
      </satisfiedBy>
      <expression xsi:type="cst:FormalLanguageExpression" id="11bdeb6d-e2b7-4be0-a289-0dec8a9175c3" language="cst:FormalConstraintLanguage platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#fr.labsticc.framework.constraints.lute" text="Property( Self, &quot;Isolette_Properties::Temperature_Increase_Rate&quot;, &quot;Fdeg_Minute&quot; ) &lt;= 0.5" usedLibraries="platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#platform:/config/fr.openpeople.ide.settings/lute/default/libStandard.lute"/>
    </requirements>
    <requirements xsi:type="core:Requirement" id="af492b68-4c1c-43f2-8d0f-45f850cb1332" name="REQ-IS-4: Heat Control Latency less Allowed Latency" description="The latency of turning the heat source on and off should be less than the allowed Heat Source Latency (6.0 sec) for the entire flow of information from the Temperature Sensor to the Heat Source." stakeHolders="#0b915e70-bb90-4791-b91e-a3a00604b739" rationale="Since a closed Isolette will warm or cool at a maximum rate of 1°F per minute (EA-IS1 and EA-IS2), turning the Heat Source on or off within 6 seconds ensures that the Current Temperature will not have changed by more than 0.1°F, the required accuracy and resolution of the Temperature Sensor (REQ-IS-1 and REQ-IS-2)." category="platform:/config/fr.openpeople.ide.settings/default.settings#cat_Performance">
      <satisfiedBy xsi:type="core:RefUserSelectedModelElements" id="4d9850ae-e9b2-4241-a103-bc80ea5a2aa5">
        <modelElementReferences id="94c8f5ff-34c8-467a-b43f-7d9f19abed8e" modelElement="../../isolette_integration_1/isolette_integration_1.aadl#isolette_integration_1::Isolette_Refined.Normal_Operation.temp_sensor_to_heat_source_flow"/>
      </satisfiedBy>
      <expression xsi:type="cst:FormalLanguageExpression" id="423f3f18-ccef-4f5a-9090-89f0cb82b64c" language="cst:FormalConstraintLanguage platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#fr.labsticc.framework.constraints.lute" text="  Upper( Actual_Latency( Self, &quot;ms&quot; ) ) &lt; Property_Constant( &quot;Isolette_Properties::Allowed_Heat_Source_Latency&quot;, &quot;ms&quot;  )&#xD;&#xA;  " usedLibraries="platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#platform:/config/fr.openpeople.ide.settings/lute/default/libStandard.lute"/>
    </requirements>
    <requirements xsi:type="core:Requirement" id="110be9af-a2a2-42eb-b8f4-14d251281324" name="REQ-IS-5: Oper Interf Latency less Max Oper Resp Time" description="The latency of displaying a change in the Operator Interface when the user change an input shall not exceed the Maximum Operator Response Time (0.5 sec).ee" stakeHolders="#0b915e70-bb90-4791-b91e-a3a00604b739" rationale="A trade study has shown that the time operatorrs wait to see the result of a request should be no more than 0.5 second as defined by the Max Operator Response Time constant." category="platform:/config/fr.openpeople.ide.settings/default.settings#cat_Performance">
      <satisfiedBy xsi:type="core:RefUserSelectedModelElements" id="0ec536f3-c928-4052-8fbd-5550b122154b">
        <modelElementReferences id="9b454a1f-8d87-45bd-aacf-431511bbf43a" modelElement="../../isolette_integration_1/isolette_integration_1.aadl#isolette_integration_1::Isolette_Refined.Normal_Operation.op_interface_to_mon_status"/>
        <modelElementReferences id="10ab7d98-00b5-4549-a74b-22e1f1fca16c" modelElement="../../isolette_integration_1/isolette_integration_1.aadl#isolette_integration_1::Isolette_Refined.Normal_Operation.op_interface_to_reg_status"/>
      </satisfiedBy>
      <expression xsi:type="cst:FormalLanguageExpression" id="8742b214-31ee-4426-ad74-138d1f3c7517" language="cst:FormalConstraintLanguage platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#fr.labsticc.framework.constraints.lute" text="  Upper( Actual_Latency( Self, &quot;ms&quot; ) ) &lt; Property_Constant( &quot;Isolette_Properties::Max_Operator_Response_Time&quot;, &quot;ms&quot;  )" usedLibraries="platform:/config/fr.openpeople.ide.settings/defaultConstraintsLanguages.constraints#platform:/config/fr.openpeople.ide.settings/lute/default/libStandard.lute"/>
