    <requirements xsi:type="core:Requirement" id="65eaa94c-6fdc-450b-8968-0b88cfa0a754" name="HR-1: Probability of Exposure to Unsafe Temp less than 10 E-9" description="The probability of prolonged exposure of Infant to unsafe heat or cold shall be less than 10E-9 per hour of operation." stakeHolders="#f38650cb-3a66-486f-b196-4ea34a7bc190 #b4d9b9e8-0b00-4728-820e-1b314e809921" rationale="The FHA for the Isolette system identified prolonged exposure of Infant to unsafe heat or cold as a catastrophic hazard." containedRequirements="#ec23fbf7-5fbd-4680-bb86-b9af4567801a #c4e29cb0-c889-4bdc-b407-43ba610eaa05 #a7f42b18-98a2-4944-848b-05ecd3e08eda" category="platform:/config/fr.openpeople.ide.settings/default.settings#cat_Safety">
      <satisfiedBy xsi:type="core:RefUserSelectedModelElements" id="83b66ffe-1fef-4111-9d83-49dce7601c40">
